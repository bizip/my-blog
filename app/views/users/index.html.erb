
<ul>
    <% @users.each do |user| %>
        <li>
            <div class="user_container">
                <div class="user_img">
                <img src=<%= user.photo %> alt="profile pic">
                </div>
                <div class="user_content">
                    <h2><%= user.name%></h1>
                    <p>Number of posts: <%= user.posts_count %></p>
                </div>
            </div>
        </li>
    <% end %>
</ul>